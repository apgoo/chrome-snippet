function sumOf(arr) {
    for(let i = 0; i < arr.length; i++) {
        for(let j = i+1; j < arr.length; j++)  {
            
            if(arr[i] + arr[j] === 0) {
                return [arr[i], arr[j]];
            }
           console.log('arrI ' + arr[i], 'arrJ ' + arr[j])
        }

    }
}

sumOf([-3, -2, -1, 0, 1, 2, 3]);